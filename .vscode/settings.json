{
    "inline-bookmarks.view.showVisibleFilesOnly": true,
    "insertDateString.format": "MM-DD-YYYY hh:mm:ss",
    "insertDateString.formatDate": "MM-DD-YYYY",
    "cSpell.words": [
        "rivt",
    ],
    "workbench.colorTheme": "Visual Studio Dark",
    "workbench.colorCustomizations": {
        "tab.activeBackground": "#2a2570",
        "tab.inactiveBackground": "#505052"
    },
    "editor.formatOnSave": true,
    "gutterpreview.imagePreviewMaxHeight": 10,
    "cSpell.suggestionMenuType": "quickFix",
    "ShortcutMenuBar.beautify": false,
    "ShortcutMenuBar.navigateBack": false,
    "ShortcutMenuBar.navigateForward": false,
    "ShortcutMenuBar.openFilesList": false,
    "ShortcutMenuBar.toggleWordWrap": true,
    "ShortcutMenuBar.saveAll": true,
    "ShortcutMenuBar.showCommands": false,
    "ShortcutMenuBar.openSettings": true,
    "ShortcutMenuBar.quickOpen": false,
    "actionButtons": {
        "defaultColor": "#ff0034",
        "reloadButton": "♻️",
        "loadNpmCommands": false,
        "commands": [
            {
                "name": "Note",
                "color": "white",
                "useVsCodeApi": false,
                "command": "code note.txt && exit",
            },
            {
                "name": "Setting",
                "color": "white",
                "useVsCodeApi": true,
                "command": "workbench.action.openSettings2",
            },
            {
                "name": "KeyBind",
                "color": "white",
                "useVsCodeApi": true,
                "command": "workbench.action.openGlobalKeybindings",
            },
            {
                "name": "ToggleSpell",
                "color": "white",
                "useVsCodeApi": true,
                "command": "cSpell.toggleEnableSpellChecker",
            },
            {
                "name": "GoTo",
                "color": "white",
                "useVsCodeApi": true,
                "command": "workbench.action.gotoLine",
            },
            {
                "name": "FoldAll",
                "color": "orange",
                "useVsCodeApi": true,
                "command": "editor.foldLevel2"
            },
            {
                "name": "Unfold",
                "color": "orange",
                "useVsCodeApi": true,
                "command": "editor.unfoldAll"
            },
            {
                "name": "ExplorerSort",
                "color": "yellow",
                "useVsCodeApi": true,
                "command": "settings.cycle.explorerSort2",
            },
        }